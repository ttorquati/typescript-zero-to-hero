{
  "compilerOptions": {
    "target": "es2019",
    "module": "commonjs",
    "lib": ["ESNext", "DOM"],
    "allowJs": true,
    "outDir": "./dist",
    "rootDir": "src",
    "strict": true,
    "noImplicitAny": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "removeComments": true,
    "noEmitOnError": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  }
}
